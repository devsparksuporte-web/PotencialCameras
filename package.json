{
  "name": "cammonitor-pro",
  "description": "Sistema de Monitoramento de Câmeras de Segurança",
  "version": "1.0.0",
  "private": true,
  "keywords": ["camera", "monitoring", "security", "surveillance"],
  "author": "CamMonitor Pro Team",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "type": "module",
  "dependencies": {
    "@hono/zod-validator": "^0.5.0",
    "hono": "4.7.7",
    "lucide-react": "^0.510.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^7.5.3",
    "recharts": "^3.2.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.0",
    "@eslint/js": "9.25.1",
    "@getmocha/vite-plugins": "latest",
    "@getmocha/users-service": "^0.0.4",
    "@types/node": "22.14.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "9.25.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.15.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "^6.2.0",
    "wrangler": "^4.33.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler publish",
    "netlify:build": "npm run build",
    "netlify:dev": "netlify dev",
    "db:migrate": "wrangler d1 migrations apply cammonitor-database",
    "db:migrate:local": "wrangler d1 migrations apply cammonitor-database --local",
    "db:create": "wrangler d1 create cammonitor-database",
    "logs": "wrangler tail",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/cammonitor-pro.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/cammonitor-pro/issues"
  },
  "homepage": "https://github.com/your-org/cammonitor-pro#readme"
}
